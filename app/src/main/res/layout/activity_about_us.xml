<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <include
        android:id="@+id/app_bar"
        layout="@layout/tittle_left" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="География пәні бойынша оқушыларға арналған мобильді қосымша 10- 11 сынып оқушыларына арналған. Бұл қосымшада сіздер дәрістерді оқып және тесттен өтіп өз білім деңгейлеріңізді көре аласыз."
        android:id="@+id/textView"
        android:layout_below="@+id/app_bar"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="5dp" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView"
        android:layout_marginLeft="5dp"
        android:text="Автор: Ибрагимова Мадина"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textStyle="bold" />




</RelativeLayout>